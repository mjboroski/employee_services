<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody id="benefits_body">
    <tr>
      <th class="sortable"><strong>Name</strong></th>
      <th><strong>Description</strong></th>
      <th><strong>Link</strong></th>
      <th class="sortable"><strong>Flat Cost/Paycheck</strong></th>
      <th class="sortable"><strong>Percent Cost</strong></th>
      <th><strong>Beneficiaries</strong></th>
      <th><strong>More...</strong></th>
    </tr>
  </tbody>
</table>

<script id="benefits-template" type="text/x-handlebars-template">
  <tr>
    <td>{{name}}</td>
    <td>{{description}}</td>
    <td>{{link}}</td>
    <td>${{flat_cost}}</td>
    <td>{{percent_cost}}%</td>
    <td>{{beneficiaries}}</td>
    <td><a href="benefits/{{id}}">{{name}} Details</td>
  </tr>
</script>
